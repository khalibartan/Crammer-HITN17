<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.radhe.schedulewish"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.radhe.schedulewish.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.radhe.schedulewish.EventDetail" />
        <activity android:name="com.example.radhe.schedulewish.ContactDisplay" />
        <activity android:name="com.example.radhe.schedulewish.SelectDate" />
        <activity android:name="com.example.radhe.schedulewish.Message" />
        <activity android:name="com.example.radhe.schedulewish.MyAlarmReciever" />

        <receiver
            android:name="com.example.radhe.schedulewish.MyAlarmReciever"
            android:enabled="true" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name="com.example.radhe.schedulewish.SMSSender" />
        <activity android:name="com.example.radhe.schedulewish.temp" />
    </application>

</manifest>